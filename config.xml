<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.jeanbrito" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Só garçonete APP</name>
    <description>Um app para garçonetes.</description>
    <author email="jeanbrito22@gmail.com" href="http://cordova.io">Jean Brito</author>
    <content src="index.html" />
    <access origin="http://cozinhapp.sergiolopes.org" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="StatusBarBackgroundColor" value="#E86C13" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^8.0.1">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-vibration" spec="^3.1.0" />
    <preference name="StatusBackgroundColor" value="#E86C13" />
    <preference name="ScrollEnabled" value="false" />
    <engine name="browser" spec="^5.0.4" />
    <engine name="android" spec="^7.1.4" />
</widget>
